{
    "name": "santos-sabanari/laravel-foundation",
    "description": "A minimalist admin panel using coreui",
    "keywords": [
        "santos-sabanari",
        "laravel-foundation"
    ],
    "homepage": "https://github.com/santos-sabanari/laravel-foundation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Santos Sabanari",
            "email": "sabanari.santos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "arcanedev/log-viewer": "8.0",
        "illuminate/support": "^8.0",
        "laravel/fortify": "^1.7",
        "livewire/livewire": "^2.3",
        "protonemedia/laravel-form-components": "^2.2",
        "santos-sabanari/laravel-datatables": "dev-develop",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-permission": "^3.17",
        "tabuna/breadcrumbs": "^2.2",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "SantosSabanari\\LaravelFoundation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SantosSabanari\\LaravelFoundation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SantosSabanari\\LaravelFoundation\\LaravelFoundationServiceProvider"
            ],
            "aliases": {
                "LaravelFoundation": "SantosSabanari\\LaravelFoundation\\LaravelFoundationFacade"
            }
        }
    }
}
